{
  "manifest_version": 2,
  "name": "FBC",
  "description": "For FBC testing.",
  "version": "1.1",
  "author": "Bao Pham",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "persistent": true
    }
  ],
  "permissions": [
    "activeTab"
  ]
}